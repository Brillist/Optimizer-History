HOST_OS		= UTL_OS_MINGW
HOST_OS_NAME	= mingw
INST_ROOT	= /opt/mingw-w64/mingw/
CONFIG_LD_OPTS	+= -Wl,--enable-auto-import -mthreads
EXT_LIBS        += /opt/mingw-w64/mingw/lib/libws2_32.a
LIBUTL_LIBDIR	= /opt/mingw-w64/mingw/lib/
LIBUTL		= $(LIBUTL_LIBDIR)$(LIBUTL_STATIC)
LIBUTL_DLL      = $(LIBUTL_BASENAME).dll
LIBUTL_DEF      = $(LIBUTL_BASENAME).def
LIBUTL_SHARED   = $(LIBUTL_BASENAME).dll.a
